  "module": "dist/intl-tel-code.es.js",
  "unpkg": "dist/intl-tel-code.umd.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist && rollup -c && npm run build:types",
